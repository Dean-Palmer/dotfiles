else{timingData.unload=0;}timingData.gaps=timing.domainLookupStart-timing.fetchStart;timingData.gaps+=timing.connectStart-timing.domainLookupEnd;timingData.gaps+=timing.requestStart-timing.connectEnd;timingData.gaps+=timing.loadEventStart-timing.domComplete;return timingData;}function emitCpuBenchmark(){var blob,worker,work,deferred=$.Deferred();if(cpuBenchmarkDone||!window.Blob||!window.URL||!window.URL.createObjectURL||!window.Worker||!window.performance){return deferred.resolve();}cpuBenchmarkDone=!0;function onMessage(){var i,startTime,amount=100000000;if(!self.performance){postMessage(false);return;}startTime=performance.now();for(i=amount;i>0;i--){}postMessage(Math.round(performance.now()-startTime));}work='onmessage = '+String(onMessage);blob=new Blob([work],{type:'application/javascript'});worker=new Worker(URL.createObjectURL(blob));deferred.then(function(result){var event;if(!result){return;}event={pageviewToken:mw.user.getPageviewToken(),score:result};mw.eventLog.logEvent(
